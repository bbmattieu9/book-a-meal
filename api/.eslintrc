{
  "extends": "airbnb-base",
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 2,
    "no-undef": 1,
    "no-return-assign": 1,
    "no-extra-parens": 2,
    "brace-style": 2,
    "semi": 2
  }
}
